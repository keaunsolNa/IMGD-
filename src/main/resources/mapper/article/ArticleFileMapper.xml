<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.nks.imgd.mapper.article.ArticleFileMapper">

    <insert id="makeArticleFile">
        INSERT
          INTO ARTICLE_FILE
          (
            ARTICLE_ID
          , FILE_ID
          )
        VALUES
          ( #{articleId}
          , #{fileId}
          )
    </insert>

</mapper>
