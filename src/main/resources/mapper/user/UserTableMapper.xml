<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.nks.imgd.mapper.user.UserTableMapper">

    <select id="findAll" resultType="com.nks.imgd.dto.userAndRole.UserTableDTO">
        SELECT USER_ID
             , NAME
             , EMAIL
             , NICK_NAME
             , PICTURE_ID
             , LOGIN_TYPE
             , LAST_LOGIN_DATE
             , REG_DTM
             , REG_ID
             , MOD_DTM
             , MOD_ID
          FROM USER_TABLE
    </select>

</mapper>
